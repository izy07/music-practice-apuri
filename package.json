{
  "name": "bolt-expo-starter",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web:clean": "expo start --web --clear",
    "web:port": "expo start --web --port 8082",
    "build:web": "npx expo export --platform web",
    "build:web:github": "EXPO_PUBLIC_WEB_BASE=/music-practice-apuri EXPO_PUBLIC_WEB_REDIRECT_URL=https://izy07.github.io/music-practice-apuri/auth/callback GITHUB_PAGES_BASE=/music-practice-apuri npx expo export --platform web || (echo '❌ Expo export failed' && exit 1) && node scripts/fix-github-pages-paths.js || (echo '❌ fix-github-pages-paths.js failed' && exit 1) && node scripts/create-robust-404.js || (echo '❌ create-robust-404.js failed' && exit 1)",
    "deploy": "bash scripts/deploy-to-gh-pages.sh",
    "build:android": "eas build --platform android",
    "build:ios": "eas build --platform ios",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:critical": "jest __tests__/integration/practiceRecord.test.ts",
    "test:ci": "CI=true GITHUB_ACTIONS=true jest --ci --coverage --maxWorkers=2",
    "test:db": "EXPO_PUBLIC_SUPABASE_URL=http://127.0.0.1:54321 EXPO_PUBLIC_SUPABASE_ANON_KEY=eyJhbGciOiJIUzI1NiIsInR5cCI6IkpXVCJ9.eyJpc3MiOiJzdXBhYmFzZS1kZW1vIiwicm9sZSI6ImFub24iLCJleHAiOjE5ODM4MTI5OTZ9.CRXP1A7WOeoJeXxjNni43kdQwgnWNReilDMblYTn_I0 jest --ci --coverage --maxWorkers=2",
    "db:setup": "bash scripts/setup-github-db.sh",
    "supabase:start": "npx supabase start",
    "supabase:stop": "npx supabase stop",
    "supabase:status": "npx supabase status",
    "supabase:reset": "npx supabase db reset",
    "supabase:seed-dev": "psql -h 127.0.0.1 -p 54322 -U postgres -d postgres < supabase/seed_users_dev.sql",
    "supabase:check": "./scripts/check-supabase.sh",
    "supabase:monitor": "./scripts/keep-supabase-alive.sh",
    "supabase:health": "./scripts/supabase-health-monitor.sh",
    "supabase:install-monitor": "cp scripts/com.music-practice.supabase-monitor.plist ~/Library/LaunchAgents/ && launchctl load ~/Library/LaunchAgents/com.music-practice.supabase-monitor.plist",
    "supabase:uninstall-monitor": "launchctl unload ~/Library/LaunchAgents/com.music-practice.supabase-monitor.plist && rm ~/Library/LaunchAgents/com.music-practice.supabase-monitor.plist",
    "supabase:logs": "tail -f logs/health-monitor.log",
    "supabase:alerts": "tail -f logs/alerts.log"
